<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.study.yuyong.peiyongdian.fragments.Realtime_state_Fragment_1">
    <android.support.design.widget.TabLayout
        android:id="@+id/tl_monitor_navigation_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/backgraound_monitor"
        app:tabIndicatorHeight="0dp"
        app:tabIndicatorColor="@color/colorPrimary"
        app:tabSelectedTextColor="@color/line_state_true"
        app:tabTextColor="@color/line_state_false"
        app:tabTextAppearance="@style/TabTextSize_1"
        >
    </android.support.design.widget.TabLayout>
    <com.study.yuyong.peiyongdian.utils.NoScrollViewPager
        android:id="@+id/monitor_show_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
    </com.study.yuyong.peiyongdian.utils.NoScrollViewPager>
</LinearLayout>